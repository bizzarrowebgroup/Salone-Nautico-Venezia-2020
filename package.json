{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "test:watch": "jest --watchAll",
    "test": "jest",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx ./app",
    "typescript": "tsc --noEmit",
    "ci": "yarn lint && yarn typescript && yarn test --coverage",
    "storybook": "start-storybook -p 7007",
    "prestorybook": "rnstl"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/jest/setup.js"
    ],
    "preset": "jest-expo",
    "collectCoverageFrom": [
      "app/**/*.(js|jsx|ts|tsx)"
    ]
  },
  "name": "salone-nautico-di-venezia-2020",
  "version": "0.0.4",
  "description": "Un app per il salone nautico di Venezia",
  "author": "BWG <info@bizzarro.org>",
  "private": true,
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "@storybook/addon-actions": "5.2.8",
    "@storybook/addon-links": "5.2.8",
    "@storybook/addons": "5.2.8",
    "@storybook/react-native": "5.2.8",
    "@storybook/react-native-server": "5.2.8",
    "@types/jest": "24.0.23",
    "@types/node": "12.12.18",
    "@types/react": "~16.9.35",
    "@types/react-native": "~0.63.2",
    "@types/react-test-renderer": "16.9.1",
    "@types/uuid": "3.4.6",
    "@typescript-eslint/eslint-plugin": "2.11.0",
    "@typescript-eslint/parser": "2.11.0",
    "babel-loader": "8.0.6",
    "babel-preset-expo": "8.3.0",
    "eslint": "6.7.2",
    "eslint-plugin-jest": "23.1.1",
    "eslint-plugin-react": "7.17.0",
    "jest-expo": "^40.0.0",
    "react-native-storybook-loader": "1.8.1"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/drawer": "^5.1.1",
    "@react-navigation/material-bottom-tabs": "^5.0.5",
    "@react-navigation/material-top-tabs": "^5.0.5",
    "@react-navigation/native": "^5.0.5",
    "@react-navigation/stack": "latest",
    "expo": "^40.0.0",
    "expo-app-loading": "^1.0.1",
    "expo-asset": "~8.2.1",
    "expo-av": "~8.7.0",
    "expo-constants": "~9.3.3",
    "expo-font": "~8.4.0",
    "expo-linear-gradient": "~8.4.0",
